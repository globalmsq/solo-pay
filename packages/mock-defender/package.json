{
  "name": "@msqpay/mock-defender",
  "version": "1.0.0",
  "description": "Mock OpenZeppelin Defender HTTP service for local gasless transaction relay",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "fastify": "^5.0.0",
    "pino-pretty": "^11.0.0",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0",
    "eslint": "^8.0.0"
  },
  "keywords": [
    "defender",
    "mock",
    "gasless",
    "blockchain",
    "relay",
    "http-service"
  ],
  "author": "MSQ Team",
  "license": "MIT"
}
